<Window x:Class="Carespot.VerwijderBevestiging"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Carespot"
        mc:Ignorable="d"
        Title="Carespot" Height="130" Width="300" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="lblVerwijderen" Content="Weet u zeker dat u dit account wilt verwijderen?" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="14,10,0,0"/>
        <Button x:Name="btnJa" Content="Ja" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="38,67,0,0" Click="btnJa_Click"/>
        <Button x:Name="btnNee" Content="Nee" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="179,67,0,0" Click="btnNee_Click"/>

    </Grid>
</Window>
